
<div class="container">
  <%= form_with(model: @event, local: true) do |form| %>
    <div class="form-group">
      <%= form.label :'Nombre Cita' %>
      <%= form.text_field :title, class: "form-control form-control-sm rounded-0" %>
    </div>

    <div class="form-group">
      <%= form.label :'Inicio' %><br>
      <%= form.datetime_select :start_time%>
    </div>

    <div class="form-group">
      <%= form.label :'Fin' %><br>
      <%= form.datetime_select :end_time %>
    </div>

    <div class="actions">
      <%= form.submit "Guardar", class: "btn btn-success btn-sm float-right" %>
    </div>
  <% end %>
</div>